{
  "social_platforms" : [
    {
      "id": 1,
      "platform": "Facebook",
      "url": "https://facebook.com/{username}",
      "method": "GET",
      "metadata": []
    },
    {
      "id": 2,
      "platform": "YouTube",
      "url": "https://www.youtube.com/user/{username}",
      "method": "GET",
      "metadata": []
    },
    {
      "id": 3,
      "platform": "Twitter",
      "url": "https://nitter.net/{username}",
      "method": "GET",
      "metadata": [
        {
          "key": "Name",
          "prefix": "",
          "search": "a[class=profile-card-fullname]",
          "output": "title"
        },
        {
          "key": "Bio",
          "prefix": "",
          "search": "div[class=profile-bio]",
          "output": ""
        },
        {
          "key": "Member Since",
          "prefix": "",
          "search": "div[class=profile-joindate]",
          "output": "title"
        },
        {
          "key": "Site",
          "prefix": "",
          "search": "div[class=profile-website]",
          "output": ""
        },
        {
          "key": "Profile Picture",
          "prefix": "https://nitter.net",
          "search": "a[class=profile-card-avatar]",
          "output": "href"
        },
        {
          "key": "Location",
          "prefix": "",
          "search": "div[class=profile-location]",
          "output": ""
        }
      ]
    },
    {
      "id": 4,
      "platform": "Telegram",
      "url": "https://t.me/{username}",
      "method": "GET",
      "metadata": [
        {
          "key": "Name",
          "prefix": "",
          "search": "span[dir=auto]",
          "output": ""
        },
        {
          "key": "Profile Picture",
          "prefix": "",
          "search": "img[class=tgme_page_photo_image]",
          "output": "src"
        }
      ]
    },
    {
      "id": 5,
      "platform": "Tiktok",
      "url": "https://www.tiktok.com/@{username}",
      "method": "GET",
      "metadata": [
        {
          "key": "Name",
          "prefix": "",
          "search": "h2[attrs={'data-e2e':'user-bio'}]",
          "output": "text"
        },
        {
          "key": "Profile Picture",
          "prefix": "",
          "search": "img",
          "output": "src"
        }
      ]
    },
    {
      "id": 6,
      "platform": "Tinder",
      "url": "https://tinder.com/@{username}",
      "method": "GET",
      "metadata": [
        {
          "key": "Name",
          "prefix": "",
          "search": "meta[property=profile:first_name]",
          "output": "content"
        },
        {
          "key": "Profile Picture",
          "prefix": "",
          "search": "meta[property=og:image]",
          "output": "content"
        }
      ]
    },
    {
      "id": 7,
      "platform": "Instagram",
      "url": "https://www.picuki.com/profile/{username}",
      "method": "GET",
      "metadata": [
        {
          "key": "Name",
          "prefix": "",
          "search": "h2[class=profile-name-bottom]",
          "output": "string"
        },
        {
          "key": "Bio",
          "prefix": "",
          "search": "div[class=profile-description]",
          "output": "text"
        },
        {
          "key": "Followers",
          "prefix": "",
          "search": "span[class=followed_by]",
          "output": "string"
        },
        {
          "key": "Following",
          "prefix": "",
          "search": "span[class=follows]",
          "output": "string"
        },
        {
          "key": "Profile Picture",
          "prefix": "",
          "search": "div[class=profile-avatar]",
          "output": "src"
        }
      ]
    }
  ]
}